@font-face {
	font-family: 'cfyeshop';
	src:url('{{ 'cfyeshop.eot?lz5y9d' | asset_url }}');
	src:url('{{ 'cfyeshop.eot?#iefixlz5y9d' | asset_url }}') format('embedded-opentype'),
		url('{{ 'cfyeshop.ttf?lz5y9d' | asset_url }}') format('truetype'),
		url('{{ 'cfyeshop.woff?lz5y9d' | asset_url }}') format('woff'),
		url('{{ 'cfyeshop.svg?lz5y9d#cfyeshop' | asset_url }}') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
